{
  "expo": {
    "name": "OrionTV",
    "slug": "oriontv",
    "version": "1.0.0",
    "scheme": "oriontv",
    "orientation": "landscape",
    "userInterfaceStyle": "dark",
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.orion.tv",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0b0c"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}